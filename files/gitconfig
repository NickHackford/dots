[alias]
    c = clone
    ca = commit --amend
    co = checkout
    fmm = fetch origin main:main
    l = log --oneline --decorate --graph --date=relative --abbrev-commit --pretty=format:'%C(yellow)%h%Creset - %s %C(red)%d%Creset %Cgreen(%cr BY %ae)%Creset'
    la = !git l --all
    pf = push origin -f
    puf = push origin -uf
    rh = reset --hard
    rb = rebase
    rbm = rebase main
    s = status
    spu = stash push -m 'temp'
    spo = stash pop
[color]
    ui = true
[core]
    autocrlf = false
    preloadindex = true
    fscache = true
    symlinks = false
    whitespace = -trailing-space,space-before-tab
[fetch]
    prune = true
[http]
    sslVerify = false
[log]
    decorate = short
[pack]
    useSparse = true
[pager]
    branch = false
[push]
    default = current
[include]
    path = ~/.gitconfig.local
[user]
    name = Nick Hackford
    email = nicholas.hackford@claconnect.com
